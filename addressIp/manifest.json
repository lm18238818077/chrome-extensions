{
  "name": "website ip",
  "manifest_version": 3,
  "version": "0.1",
  "description": "show website remote address",
  "action": {
    "default_title": "website ip",
    "default_icon": "icon.png"
  },
  "permissions": [
    "webRequest",
    "webNavigation", 
    "tabs",
    "scripting",
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*/",
    "http://*/*",
    "https://*/*"
  ]
}
